{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341/",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    },
    "AllowedHosts": "*"
  },
  "Swagger": {
    "ApiVersion": "1.0.0",
    "ApiName": "Cooking App",
    "JsonRoute": "swagger/{documentName}/swagger.json"
  }
}
